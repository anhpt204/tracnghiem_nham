package pbts.simulation;

public class Window {

	/**
	 * @param args
	 */
	public double minLat;
	public double maxLat;
	public double minLng;
	public double maxLng;
	public Window(double minLat, double maxLat, double minLng, double maxLng){
		this.minLat = minLat;
		this.maxLat = maxLat;
		this.minLng = minLng;
		this.maxLng = maxLng;
	}
	public boolean contains(double lat, double lng){
		return minLat <= lat && lat <= maxLat && minLng <= lng && lng <= maxLng;
	}
